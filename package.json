{
  "name": "sctools",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "cljs": "clojure -A:shadow-cljs -M:shadow-cljs",
    "watch": "yarn cljs watch --debug :app",
    "test": "yarn cljs compile :test",
    "ci-build": "yarn cljs release :ci",
    "ci-test": "karma start --single-run",
    "compile-css": "yarn postcss --verbose static/styles/tailwind/tailwind-main.css static/styles/tailwind/tailwind-custom.css --dir assets/app/sctools/static/dev/styles/",
    "watch-css": "yarn compile-css --watch",
    "purge-fa-css": "yarn postcss resources/app/sctools/static/fontawesome/css/all.min.css -o resources/app/sctools/static/fontawesome/css/fa.purged.css --env prod",
    "release-js": "yarn cljs release :app",
    "release-tailwind-css": "yarn postcss --verbose static/styles/tailwind/tailwind-main.css static/styles/tailwind/tailwind-custom.css --dir resources/app/sctools/static/styles/ --env prod",
    "release-css": "yarn release-tailwind-css; yarn purge-fa-css",
    "release": "yarn release-css; yarn release-js",
    "restart-js": "goreman run restart js",
    "start": "goreman start"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@testing-library/react": "^12.1.2",
    "@types/debug": "^4.1.7",
    "autoprefixer": "^10.4.0",
    "create-react-class": "^15.7.0",
    "cssnano": "^5.0.8",
    "cypress": "8.7.0",
    "karma": "^6.3.6",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cljs-test": "^0.1.0",
    "marked": "^3.0.8",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.1",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^6.7.0",
    "puppeteer": "10.4.0",
    "shadow-cljs": "2.15.12",
    "source-map-support": "^0.5.20",
    "stylelint": "^14.0.1",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.4.4",
    "ws": "^8.2.3"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@js-joda/core": "4.3.1",
    "@js-joda/locale_en-us": "4.4.0",
    "@js-joda/timezone": "2.10.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@popperjs/core": "^2.10.2",
    "highlight.js": "^11.3.1",
    "idb": "^6.1.5",
    "material-table": "^1.69.3",
    "moment": "^2.29.1",
    "platform": "1.3.6",
    "react": "^17.0.2",
    "react-devtools-core": "^4.20.2",
    "react-dom": "^17.0.2",
    "react-highlight.js": "1.0.7",
    "react-refresh": "^0.10.0",
    "react-router-dom": "^5.3.0",
    "react-spring": "^9.3.0",
    "react-transition-group": "^4.4.2",
    "react-vega": "^7.4.4",
    "stack-trace": "1.0.0-pre1",
    "stacktrace-js": "2.0.2",
    "vega": "5.20.2",
    "vega-embed": "6.15.2",
    "vega-lite": "5.1.0",
    "vega-tooltip": "0.25.0",
    "xregexp": "^5.1.0"
  },
  "resolutions": {
    "vega-functions": "5.11.0",
    "vega-parser": "6.1.3",
    "vega-event-selector": "2.0.6",
    "vega-dataflow": "5.7.4"
  }
}
