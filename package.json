{
  "name": "sctools",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "cljs": "clojure -A:shadow-cljs -M:shadow-cljs",
    "watch": "yarn cljs watch --debug :app",
    "test": "yarn cljs compile :test",
    "ci-build": "yarn cljs release :ci",
    "ci-test": "karma start --single-run",
    "compile-css": "yarn postcss --verbose static/styles/tailwind/tailwind-main.css static/styles/tailwind/tailwind-custom.css --dir assets/app/sctools/static/dev/styles/",
    "watch-css": "yarn compile-css --watch",
    "purge-fa-css": "yarn postcss resources/app/sctools/static/fontawesome/css/all.min.css -o resources/app/sctools/static/fontawesome/css/fa.purged.css --env prod",
    "release-js": "yarn cljs release :app",
    "release-tailwind-css": "yarn postcss --verbose static/styles/tailwind/tailwind-main.css static/styles/tailwind/tailwind-custom.css --dir resources/app/sctools/static/styles/ --env prod",
    "release-css": "yarn release-tailwind-css; yarn purge-fa-css",
    "release": "yarn release-css; yarn release-js",
    "restart-js": "goreman run restart js",
    "start": "goreman start"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "@testing-library/react": "^10.0.4",
    "@types/debug": "^4.1.5",
    "autoprefixer": "^9.7.6",
    "create-react-class": "^15.6.3",
    "cssnano": "^4.1.10",
    "cypress": "8.2.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cljs-test": "^0.1.0",
    "marked": "^1.1.0",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.2.4",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "postcss-preset-env": "^6.7.0",
    "puppeteer": "2.1.1",
    "shadow-cljs": "2.15.6",
    "source-map-support": "^0.5.13",
    "stylelint": "^13.6.1",
    "tailwindcss": "^2.0.0",
    "typescript": "^4.3.2",
    "ws": "^7.1.2"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^10.0.27",
    "@js-joda/core": "3.1.0",
    "@js-joda/locale_en-us": "3.1.1",
    "@js-joda/timezone": "2.3.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@popperjs/core": "^2.4.4",
    "highlight.js": "^10.0.3",
    "idb": "^5.0.4",
    "material-table": "^1.69.0",
    "moment": "^2.25.3",
    "platform": "1.3.6",
    "react": "^16.13.1",
    "react-devtools-core": "^4.2.0",
    "react-dom": "^16.13.1",
    "react-highlight.js": "1.0.7",
    "react-refresh": "^0.8.1",
    "react-router-dom": "^5.2.0",
    "react-spring": "^8.0.27",
    "react-transition-group": "^4.4.1",
    "react-vega": "^7.4.1",
    "stack-trace": "0.0.10",
    "stacktrace-js": "2.0.2",
    "vega": "^5.15.0",
    "vega-embed": "^6.11.1",
    "vega-lite": "^4.14.1",
    "vega-tooltip": "^0.23.2",
    "xregexp": "^4.2.4"
  }
}
